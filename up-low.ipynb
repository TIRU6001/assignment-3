    "def up_low(s):      \n",
    "    u = 0\n",
    "    l = 0\n",
    "    for i in s:\n",
    "        if i.isupper():\n",
    "            u+=1\n",
    "        else :\n",
    "            l+=1\n",
    " \n",
    "        \n",
    "    print( \"No. of Upper case characters : %s,No. of Lower case characters : %s\" % (u,l))\n",
    "\n",
    "up_low(\"Aku.Thirupathi\")"
   ]
